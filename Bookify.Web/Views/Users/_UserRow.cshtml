@model ApplicationUserViewModel
<tr>
    <td>@Model.Email</td>
    <td>@Model.FullName</td>
    <td>@Model.CreateOn.ToString("dd, MMM yyyy")</td>
    <td class="js-update-on">@(Model.LastUpdatedOn is null ? string.Empty : ((DateTime)Model.LastUpdatedOn).ToString("dd, MMM yyyy"))</td>
    <td>
        <span class="badge bg-label-@(Model.IsDeleted ? "danger" : "success") js-status">
            @(Model.IsDeleted ? "Deleted" : "Available")
        </span>
    </td>
    <td>
        <div class="dropdown">
            <a class="btn p-0 dropdown-toggle hide-arrow" data-bs-toggle="dropdown">
                <i class="bx bx-dots-vertical-rounded"></i>
            </a>
            <div class="dropdown-menu">
                <a class="dropdown-item js-render-modal" href="javascript:;" data-updated="true" data-title="Edit Author" data-url="/Users/Edit/@Model.Id"><i class="bx bx-edit-alt me-1"></i>Edit</a>
                <a class="dropdown-item js-render-modal" href="javascript:;" data-title="Reset Password" data-table="false"
                data-url="/Users/ResetPassword/@Model.Id">
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-key" viewBox="0 0 16 16">
                        <path d="M0 8a4 4 0 0 1 7.465-2H14a.5.5 0 0 1 .354.146l1.5 1.5a.5.5 0 0 1 0 .708l-1.5 1.5a.5.5 0 0 1-.708 0L13 9.207l-.646.647a.5.5 0 0 1-.708 0L11 9.207l-.646.647a.5.5 0 0 1-.708 0L9 9.207l-.646.647A.5.5 0 0 1 8 10h-.535A4 4 0 0 1 0 8m4-3a3 3 0 1 0 2.712 4.285A.5.5 0 0 1 7.163 9h.63l.853-.854a.5.5 0 0 1 .708 0l.646.647.646-.647a.5.5 0 0 1 .708 0l.646.647.646-.647a.5.5 0 0 1 .708 0l.646.647.793-.793-1-1h-6.63a.5.5 0 0 1-.451-.285A3 3 0 0 0 4 5" />
                        <path d="M4 8a1 1 0 1 1-2 0 1 1 0 0 1 2 0" />
                    </svg> Reset Password
                </a>
                <a class="dropdown-item js-toggle-status" href="javascript:;" data-url="/Users/ToggleStatus/@Model.Id"><i class="bx bx-trash me-1"></i>Toggle Status</a>
                <a class="dropdown-item js-confirm" href="javascript:;" data-url="/Users/UnLockUser/@Model.Id" data-message="Are you sure to unlock user ?">
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-unlock" viewBox="0 0 16 16">
                        <path d="M11 1a2 2 0 0 0-2 2v4a2 2 0 0 1 2 2v5a2 2 0 0 1-2 2H3a2 2 0 0 1-2-2V9a2 2 0 0 1 2-2h5V3a3 3 0 0 1 6 0v4a.5.5 0 0 1-1 0V3a2 2 0 0 0-2-2M3 8a1 1 0 0 0-1 1v5a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1V9a1 1 0 0 0-1-1z" />
                    </svg> UnLock User
                </a>
            </div>
        </div>
    </td>
</tr>